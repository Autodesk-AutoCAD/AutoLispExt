{
	"comments": {
		"blockComment": [ ";|", "|;" ],
		"lineComment": ";",
	},
    "brackets": [
		["(", ")"],
		["[", "]"]
    ],
    "autoClosingPairs": [
		{"open": "(", "close": ")"},
		{"open": "[", "close": "]"},
		{"open": "\"","close": "\"", "notIn": ["string"]},
		{"open": "#|", "close": "|#"},
		{"open": ";|", "close": "|;"}
    ],
    "surroundingPairs": [
		["\"", "\""],
		["(", ")"],
		["[","]"],
		["#|", "|#"],
		[";|", "|;"]
	],
	"folding": {
	  "markers": {
		"start": "^\\s*;\\s*#?region\\b",
		"end": "^\\s*;\\s*#?endregion\\b"
	  }
	}
}